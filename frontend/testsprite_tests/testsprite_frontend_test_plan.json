{
    "project_name": "auto-dashboard-frontend",
    "test_type": "frontend",
    "base_url": "http://localhost:3000",
    "tests": [
        {
            "id": "FE-TC001",
            "title": "Login Page Loads Successfully",
            "description": "Verify that the login page loads with all required elements",
            "steps": [
                "Navigate to /login",
                "Verify login form is visible",
                "Verify email input field exists",
                "Verify password input field exists",
                "Verify login button is present"
            ],
            "expected_result": "Login page loads with all form elements visible"
        },
        {
            "id": "FE-TC002",
            "title": "User Login with Valid Credentials",
            "description": "Test successful user login flow",
            "steps": [
                "Navigate to /login",
                "Enter valid email address",
                "Enter valid password",
                "Click login button",
                "Verify redirect to dashboard"
            ],
            "expected_result": "User is logged in and redirected to dashboard"
        },
        {
            "id": "FE-TC003",
            "title": "Dashboard Page Access After Login",
            "description": "Verify dashboard displays correctly after authentication",
            "steps": [
                "Login with valid credentials",
                "Navigate to /dashboard",
                "Verify balance card is visible",
                "Verify navigation menu is present"
            ],
            "expected_result": "Dashboard loads with user data"
        },
        {
            "id": "FE-TC004",
            "title": "Settings Page Navigation",
            "description": "Test navigation to settings page",
            "steps": [
                "Login with valid credentials",
                "Navigate to /settings",
                "Verify API key section is visible",
                "Verify profile section is visible"
            ],
            "expected_result": "Settings page loads with all sections"
        },
        {
            "id": "FE-TC005",
            "title": "Trading Page Chart Display",
            "description": "Verify trading chart loads correctly",
            "steps": [
                "Login with valid credentials",
                "Navigate to /trading",
                "Verify chart component is visible",
                "Verify order form is present"
            ],
            "expected_result": "Trading page displays chart and order form"
        },
        {
            "id": "FE-TC006",
            "title": "Protected Route Redirect",
            "description": "Verify unauthenticated users are redirected to login",
            "steps": [
                "Clear authentication tokens",
                "Attempt to navigate to /dashboard",
                "Verify redirect to /login"
            ],
            "expected_result": "Unauthenticated user redirected to login page"
        },
        {
            "id": "FE-TC007",
            "title": "Bot Management Page",
            "description": "Test bot management page functionality",
            "steps": [
                "Login with valid credentials",
                "Navigate to /bots",
                "Verify bot status display",
                "Verify start/stop controls are visible"
            ],
            "expected_result": "Bot management page displays correctly"
        },
        {
            "id": "FE-TC008",
            "title": "User Logout",
            "description": "Test user logout functionality",
            "steps": [
                "Login with valid credentials",
                "Click logout button",
                "Verify redirect to login page",
                "Verify tokens are cleared"
            ],
            "expected_result": "User is logged out and redirected to login"
        }
    ],
    "test_credentials": {
        "email": "testuser@example.com",
        "password": "TestPassword123!"
    }
}