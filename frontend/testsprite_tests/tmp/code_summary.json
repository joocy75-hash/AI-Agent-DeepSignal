{
    "tech_stack": [
        "React 18.x",
        "Vite 7.x",
        "Ant Design 5.x",
        "Axios",
        "React Router 6.x",
        "Lightweight Charts",
        "Recharts",
        "TailwindCSS"
    ],
    "features": [
        {
            "name": "Login Page",
            "description": "User authentication with email/password, social OAuth (Google, Kakao), 2FA support",
            "files": [
                "src/pages/Login.jsx",
                "src/api/auth.js",
                "src/context/AuthContext.jsx"
            ],
            "route": "/login"
        },
        {
            "name": "Dashboard",
            "description": "Main dashboard with balance overview, position summary, bot status, and quick actions",
            "files": [
                "src/pages/Dashboard.jsx",
                "src/components/BalanceCard.jsx",
                "src/components/PositionList.jsx"
            ],
            "route": "/dashboard"
        },
        {
            "name": "Trading Page",
            "description": "Live trading interface with chart, order form, and position management",
            "files": [
                "src/pages/Trading.jsx",
                "src/components/TradingChart.jsx",
                "src/api/order.js"
            ],
            "route": "/trading"
        },
        {
            "name": "Settings Page",
            "description": "User settings including API key configuration, 2FA setup, risk settings, and profile",
            "files": [
                "src/pages/Settings.jsx",
                "src/components/settings/TwoFactorSetup.jsx",
                "src/api/account.js"
            ],
            "route": "/settings"
        },
        {
            "name": "Bot Management",
            "description": "Trading bot control - start/stop bots, view status, configure strategies",
            "files": [
                "src/pages/BotManagement.jsx",
                "src/api/bot.js",
                "src/api/botInstances.js"
            ],
            "route": "/bots"
        },
        {
            "name": "Strategy Page",
            "description": "Trading strategy management and configuration",
            "files": [
                "src/pages/Strategy.jsx",
                "src/api/strategy.js",
                "src/context/StrategyContext.jsx"
            ],
            "route": "/strategy"
        },
        {
            "name": "Backtesting Page",
            "description": "Strategy backtesting with historical data analysis and comparison",
            "files": [
                "src/pages/BacktestingPage.jsx",
                "src/pages/BacktestHistoryPage.jsx",
                "src/api/backtest.js"
            ],
            "route": "/backtesting"
        },
        {
            "name": "Trading History",
            "description": "View past trades, order history, and performance analytics",
            "files": [
                "src/pages/TradingHistory.jsx",
                "src/api/trades.js",
                "src/api/analytics.js"
            ],
            "route": "/history"
        },
        {
            "name": "Notifications",
            "description": "System notifications and alerts management",
            "files": [
                "src/pages/Notifications.jsx",
                "src/components/NotificationBell.jsx",
                "src/api/alerts.js"
            ],
            "route": "/notifications"
        },
        {
            "name": "OAuth Callback",
            "description": "Social login callback handler for Google/Kakao OAuth",
            "files": [
                "src/pages/OAuthCallback.jsx"
            ],
            "route": "/oauth/callback"
        }
    ],
    "components": [
        {
            "name": "TradingChart",
            "description": "Real-time candlestick chart with position markers using Lightweight Charts",
            "file": "src/components/TradingChart.jsx"
        },
        {
            "name": "PositionList",
            "description": "Display open positions with P&L and close actions",
            "file": "src/components/PositionList.jsx"
        },
        {
            "name": "BalanceCard",
            "description": "Account balance display with USDT breakdown",
            "file": "src/components/BalanceCard.jsx"
        },
        {
            "name": "ConnectionStatus",
            "description": "WebSocket and API connection status indicator",
            "file": "src/components/ConnectionStatus.jsx"
        },
        {
            "name": "ErrorBoundary",
            "description": "React error boundary for graceful error handling",
            "file": "src/components/ErrorBoundary.jsx"
        },
        {
            "name": "MainLayout",
            "description": "Main application layout with sidebar navigation",
            "file": "src/components/layout/MainLayout.jsx"
        }
    ],
    "contexts": [
        {
            "name": "AuthContext",
            "description": "Authentication state management with JWT and refresh token",
            "file": "src/context/AuthContext.jsx"
        },
        {
            "name": "WebSocketContext",
            "description": "WebSocket connection management for real-time data",
            "file": "src/context/WebSocketContext.jsx"
        },
        {
            "name": "ThemeContext",
            "description": "Dark/light theme management",
            "file": "src/context/ThemeContext.jsx"
        },
        {
            "name": "StrategyContext",
            "description": "Trading strategy state management",
            "file": "src/context/StrategyContext.jsx"
        }
    ],
    "api_endpoints": {
        "auth": [
            "/auth/login",
            "/auth/register",
            "/auth/refresh",
            "/auth/change-password",
            "/auth/2fa/*"
        ],
        "account": [
            "/account/balance",
            "/account/positions",
            "/account/save_keys",
            "/account/my_keys"
        ],
        "bot": [
            "/bot/start",
            "/bot/stop",
            "/bot/status"
        ],
        "order": [
            "/order/submit",
            "/order/close_position",
            "/order/open",
            "/order/history"
        ],
        "chart": [
            "/chart/candles/{symbol}",
            "/chart/positions/{symbol}",
            "/chart/status"
        ],
        "strategy": [
            "/strategy/list",
            "/strategy/create",
            "/strategy/update/{id}"
        ]
    }
}